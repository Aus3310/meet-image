{
  "name": "meet-image-viewer",
  "version": "1.2",
  "manifest_version": 2,
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "description": "Render images from URL messages in Google Meets chat",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://meet.google.com/*"],
      "js": ["index.js"]
    }
  ]
}
