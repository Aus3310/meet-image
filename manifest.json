{
  "name": "meet-image-viewer",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "description": "Image view on google meet chats",
  "permissions": ["tabs"],
  "content_scripts": [
    {
      "matches": ["https://meet.google.com/*"],
      "js": ["background.js"]
    }
  ]
}
